{
    "agent": {
        "continuous": false,
        "n_actions": 5,
        "lr": 0.0001,
        "gamma": 0.99,
        "epsilon_start": 1.0,
        "epsilon_end": 0.01,
        "epsilon_decay": 125000,
        "buffer_size": 100000,
        "batch_size": 32,
        "target_update_freq": 1000,
        "learning_starts": 1000
    },
    "env": {
        "frame_stack": 4,
        "skip_frames": 2
    },
    "training": {
        "max_episodes": 900,
        "target_score": 750,
        "convergence_episodes": 100,
        "convergence_threshold": 10,
        "save_frequency": 100,
        "eval_frequency": 100,
        "eval_episodes": 10
    },
    "logging": {
        "log_dir": "logs",
        "use_tensorboard": true
    },
    "checkpoints": {
        "save_dir": "checkpoints"
    },
    "seed": 42,
    "device": "cpu"
}